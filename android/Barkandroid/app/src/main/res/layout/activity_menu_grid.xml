<?xml version="1.0" encoding="utf-8"?>
<android.support.percent.PercentRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".menu_grid"
    android:background="@color/myColor">


    <ImageView

        app:srcCompat="@mipmap/dog_loguot_icon_new2"
        android:id="@+id/logouticon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        app:layout_widthPercent="10%"
        app:layout_marginTopPercent="0%"
        app:layout_marginBottomPercent="0%"
        />


    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_profile_icon_new"
        android:id="@+id/ImageProfile"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="5%"/>

    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_map_icon_new"
        android:id="@+id/imageMap"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="5%"/>

    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_search_icon_new"
        android:id="@+id/userList"
        android:layout_alignParentStart="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="35%"/>

    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_vaccines_icon_new"
        android:id="@+id/imageView5"
        android:layout_alignParentEnd="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="35%"/>

    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_walk_icon_new"
        android:id="@+id/walklist"
        android:layout_alignParentStart="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="66%"
        app:layout_marginBottomPercent="0%"/>

    <ImageView
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@mipmap/dog_chat_icon_new"
        android:id="@+id/Chat_icon"
        android:layout_alignParentEnd="true"
        app:layout_widthPercent="50%"
        app:layout_marginTopPercent="66%"
        app:layout_marginBottomPercent="0%"
        />






</android.support.percent.PercentRelativeLayout>
